{
    "name": "sylius/admin-ui",
    "type": "library",
    "require": {
        "php": "^8.1",
        "knplabs/knp-menu-bundle": "^3.0",
        "sylius/twig-hooks": "^0.3 || dev-main",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/twig-bundle": "^6.4 || ^7.0",
        "sylius/resource-bundle": "^1.11 || ^1.12@alpha",
        "twig/twig": "^2.15 || ^3.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^5.1",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^6.4 || ^7.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/css-selector": "^6.4 || ^7.0",
        "symfony/dotenv": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/runtime": "^6.4 || ^7.0",
        "symfony/yaml": "^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sylius\\AdminUi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sylius\\TwigHooks\\": "../TwigHooks/src/",
            "TestApplication\\Sylius\\AdminUi\\": "tests/Functional/.application/src/",
            "Tests\\Sylius\\AdminUi\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*/*"
        }
    ],
    "extra": {
        "symfony": {
            "require": "7.0.*"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
